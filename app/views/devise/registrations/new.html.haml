.header_wrapper
  = image_tag 'Rectangle.png'
.container
  = form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f|
    = render "devise/shared/error_messages", resource: resource
    .field
      = f.label :account_name, 'Account', class: 'label'
      %br/
      = f.text_field :account_name, class: 'text'
    .field
      = f.label :email, class: 'label'
      %br/
      = f.email_field :email, autofocus: true, autocomplete: "email", class: 'text'
    .field
      = f.label :password, 'Password', class: 'label'
      %br/
      = f.password_field :password, autocomplete: "new-password", class: 'text'
    .actions
      = f.submit "CREATE ACCOUNT", class: 'user_btn'
  -# = render "devise/shared/links"
